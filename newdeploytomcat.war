node {
   def mvnHome
   stage('Preparation') {
      git url: 'https://github.com/patelapeksha/my-app.git', branch: 'master'
      mvnHome = tool 'Maven'
   }
   stage('Build') {
      sh "'${mvnHome}/bin/mvn' -Dmaven.test.failure.ignore clean package"
   }
   stage('Results') {
      archive 'target/*.jar'
   }
}
